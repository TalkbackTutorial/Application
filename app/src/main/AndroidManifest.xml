<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.talkbacktutorial">

    <queries>
        <intent>
            <action android:name="android.intent.action.TTS_SERVICE" />
        </intent>
    </queries>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TalkbackTutorial">
        <activity
            android:name=".activities.lesson0.Lesson0Activity"
            android:exported="false" />
        <activity
            android:name=".activities.modules.TodoModuleYModuleActivity"
            android:exported="false" />
        <activity
            android:name=".activities.modules.TodoModuleXModuleActivity"
            android:exported="false" />
        <activity
            android:name=".activities.LessonActivity"
            android:exported="false" />
        <activity
            android:name=".activities.modules.adjustreadingcontrols.AdjustReadingControlsActivity"
            android:exported="false" />
        <activity
            android:name=".activities.modules.jumptext.JumpTextActivity"
            android:exported="false" />
        <activity
            android:name=".activities.modules.jumpcontrols.JumpControlsActivity"
            android:exported="false" />
        <activity
            android:name=".activities.modules.jumpheaders.JumpHeadersActivity"
            android:exported="false" />
        <activity
            android:name=".activities.modules.jumplinks.JumpLinksActivity"
            android:exported="false" />
        <activity
            android:name=".activities.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>